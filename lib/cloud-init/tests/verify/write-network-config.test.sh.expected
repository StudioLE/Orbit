#cloud-config

network:
  version: 2
  ethernets:
    eth0:
      set-name: eth0
      match:     
        name: en*
      addresses:
        - 10.10.10.253/24
      nameservers:
        addresses:
          - 10.10.10.1
          - 1.1.1.1
          - 1.0.0.1
        search:
          - local
      routes:
        - to: default
          via: 10.10.10.1
